        location = /v1/transaction/getunsigned { rewrite ^ /v1/transaction/getunsigned; }
        location /v1/transaction/getunsigned { try_files $uri @flask_tx_generate_service; }
        location @flask_tx_generate_service {
                include uwsgi_params;
